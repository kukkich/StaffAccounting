@{
    ViewData["Title"] = "Home Page";
}
@model IEnumerable<StaffAccounting.Models.Company.Employee>

<table class="table">
    <tr>
        <td>Id</td>
        <td>FullName</td>
        <td>Birthday</td>
        <td>Sex</td>
        <td></td>
    </tr>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.Id</td>
            <td>@employee.FullName</td>
            <td>@employee.Birthday</td>
            <td>@employee.Sex</td>
            <td>
                <a asp-action="Details" asp-controller="Employee" asp-route-id="@employee.Id">Подробнее</a> |
            </td>
        </tr>
    }
</table>
